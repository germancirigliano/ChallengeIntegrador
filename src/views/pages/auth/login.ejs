<main class="funkoshop-page-container">
  <section>
    <section class="login-container">
      <h1 class="login-title"> INGRESAR A MI CUENTA</h1>
      <form class="" action="" method="post">
        <div class="login-form-box--grid">
          <label for="email">Email:</label>
          <input type="email" name="email" id="email" placeholder="johndoe@correo.com"><br>
        </div>
        <div class="login-form-box--grid">
          <label for="password">Contraseña:</label>
          <input type="password" name="password" id="password" placeholder="*********"><br>
        </div>
        
         <div class="login-form__submission">
          <input class="login-form--btn" type="submit" value="Ingresar">
          <input type="checkbox" name="términos" id="términos">
          <label class="register-form--accept" for="términos">Recordarme<a class="register-form--term" href=""></a></label>
        </div><a href="">Olvidé mi contraseña</a>
      </form>
      
    </section>
  </section>
